{
    "name": "wp-starter",
    "version": "1.0.0",
    "description": "A sweet little WordPress starter kit.",
    "main": "index.js",
    "engines": {
      "node": ">=8 <10"
    },
    "scripts": {
      "preinstall": "composer install",
      "install:wp": "node scripts/install.js",
      "uninstall:wp": "vendor/bin/wp db reset",
      "dev:wp": "cross-env DEV=true vendor/bin/wp server --docroot=web --host=0.0.0.0 --port=3000 --color --quiet",
      "dev:webpack": "webpack-dev-server --config webpack/webpack.config.js --colors --info=false",
      "dev": "npm-run-all --parallel dev:wp dev:webpack",
      "clean": "node scripts/clean.js",
      "build": "npm run clean && webpack --config webpack/webpack.config.js",
      "eslint": "eslint scripts src",
      "stylelint": "stylelint \"src/**/*.scss\" --syntax scss",
      "lint": "npm run eslint; npm run stylelint",
      "test": "npm run lint"
    },
    "author": "Megan Copper",
    "license": "ISC",
    "dependencies": {
      "chalk": "2.3.0",
      "dotenv": "4.0.0",
      "app-root-dir": "1.0.2",
      "rimraf": "2.6.2"
    },
    "devDependencies": {
      "autoprefixer": "7.1.6",
      "babel-core": "6.26.0",
      "babel-loader": "7.1.2",
      "babel-preset-env": "1.6.1",
      "babel-preset-stage-0": "6.24.1",
      "copy-webpack-plugin": "4.2.1",
      "cross-env": "5.1.1",
      "css-loader": "0.28.7",
      "eslint": "4.11.0",
      "extract-text-webpack-plugin": "3.0.2",
      "node-sass": "^4.12.0",
      "npm-run-all": "4.1.2",
      "postcss-csso": "3.0.0",
      "postcss-loader": "2.0.8",
      "sass-loader": "6.0.6",
      "style-loader": "0.19.0",
      "stylelint": "8.2.0",
      "webpack": "3.8.1",
      "webpack-dev-server": "2.9.4",
      "write-file-webpack-plugin": "4.2.0"
    }
  }
  